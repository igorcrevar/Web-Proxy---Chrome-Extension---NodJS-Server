{
  "name": "My First Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://api.flickr.com/",
	"http://ajax.googleapis.com/",
	 "webRequest", "webRequestBlocking", "<all_urls>"
  ]
}